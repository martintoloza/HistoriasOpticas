<p id="notice"><%= notice %></p>

<h1>Listing Ciudades</h1>

<table>
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ciudades.each do |ciudad| %>
      <tr>
        <td><%= ciudad.codigo %></td>
        <td><%= ciudad.nombre %></td>
        <td><%= link_to t("helpers.links.Show"), ciudad %></td>
        <td><%= link_to t("helpers.links.Edit"), edit_ciudad_path(ciudad) %></td>
        <td><%= link_to t('helpers.links.Destroy'), ciudad, method: :delete, data: { confirm: t('helpers.links.Confirm') } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t("helpers.links.Newa") + ' Ciudad', new_ciudad_path %>
